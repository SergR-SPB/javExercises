package levelUP3.Calculator;
// sum
public class CalculatorVar2 {
    double sum (double a,double b ){
        double calculate;
        calculate = a+b;
        return calculate;
    }
    int sum (int a,int b ){
        int calculate;
        calculate = a+b;
        return calculate;
    }
    long sum (long a,long b ){
        long calculate;
        calculate = a+b;
        return calculate;
    }
    // subtraction
    double subtraction (double a,double b ){
        double calculate;
        calculate = a-b;
        return calculate;
    }
    int subtraction (int a,int b ){
        int calculate;
        calculate = a-b;
        return calculate;
    }
    long subtraction (long a,long b ){
        long calculate;
        calculate = a-b;
        return calculate;
    }
    //multiplication
    double multiplication (double a,double b ){
        double calculate;
        calculate = a*b;
        return calculate;
    }
    int multiplication (int a,int b ){
        int calculate;
        calculate = a*b;
        return calculate;
    }
    long multiplication (long a,long b ){
        long calculate;
        calculate = a*b;
        return calculate;
    }
    //division
    double division (double a,double b ){
        double calculate;
        calculate = a/b;
        return calculate;
    }
    int division (int a,int b ){
        int calculate;
        calculate = a/b;
        return calculate;
    }
    long division (long a,long b ){
        long calculate;
        calculate = a/b;
        return calculate;
    }


}

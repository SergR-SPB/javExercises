package levelUP3.Calculator;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        CalculatorVar2 calculatorVar2 = new CalculatorVar2();

        System.out.println("Pezult sum: " + calculatorVar2.sum(5, 4));
        System.out.println("Pezult subtraction: " + calculatorVar2.subtraction(5.02, 4));
        System.out.println("Pezult multiplication: " + calculatorVar2.multiplication(5000, 4.2));
        System.out.println("Pezult multiplication: " + calculatorVar2.division(5, 4.02));
    }
}



